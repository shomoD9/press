{
  "name": "press-v1-animation-engine",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Press V1 deterministic programmatic animation engine.",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "press": "node --import tsx scripts/press.ts",
    "validate": "node --import tsx scripts/press.ts validate artifacts/projects/demo-v1/project.spec.json",
    "preview": "node --import tsx scripts/press.ts preview --project demo-v1 --scene all",
    "render:scene": "node --import tsx scripts/press.ts render:scene --project demo-v1 --scene beat-01-thesis",
    "render:full": "node --import tsx scripts/press.ts render:full --project demo-v1",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@remotion/bundler": "4.0.0",
    "@remotion/cli": "4.0.0",
    "@remotion/renderer": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remotion": "4.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.16.5",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.24",
    "tsx": "4.19.1",
    "typescript": "5.6.3",
    "vitest": "2.1.1"
  }
}
